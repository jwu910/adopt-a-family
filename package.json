{
  "name": "adopt-a-family",
  "version": "0.1.0",
  "description": "basic routes",
  "main": "app.js",
  "scripts": {
    "build": "react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/devBuild",
    "format": "prettier --write */**/*.js",
    "sass-lint": "sass-lint ./src --config .sass-lint.yml 'src/**/*.scss' --verbose --ignore 'node_modules/**/*'",
    "server": "nodemon server/app.js",
    "start": "concurrently --prefix \"[AAF]\" \"npm run watch-css\" \"npm run server\" \"react-scripts start\"",
    "eslint": "eslint ./",
    "test:webapp": "jest src/",
    "test:api": "jest Api --notify --env=node --forceExit",
    "test:all": "concurrently 'npm run test:webapp' 'npm run test:api'",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/devBuild/ --watch --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/5-gwoap/adopt-a-family.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/5-gwoap/adopt-a-family/issues"
  },
  "homepage": "https://test-aaf.herokuapp.com/",
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/server/"
    ],
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testURL": "http://localhost",
    "timers": "fake"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.1",
    "mongoose": "^4.13.11",
    "prop-types": "^15.6.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@types/autobind-decorator": "^2.1.0",
    "@types/classnames": "^2.2.9",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.9",
    "@types/react-router-dom": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "autobind-decorator": "^2.4.0",
    "autoprefixer": "^9.6.5",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.3.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^3.5.1",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-sort-destructure-keys": "^1.3.3",
    "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
    "jest": "^24.9.0",
    "jest-cli": "^22.4.4",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.14.12",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.14.2",
    "react-test-renderer": "^16.2.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.3.1",
    "superagent": "^3.8.2",
    "supertest": "^3.0.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  },
  "proxy": "http://localhost:3005/",
  "sasslintConfig": "./.sass-lint.yml"
}
